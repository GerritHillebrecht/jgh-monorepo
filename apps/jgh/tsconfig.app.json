{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "../../dist/out-tsc",
    "types": [],
    "paths": {
      "@jgh/*": ["apps/jgh/src/app/*"],
      "@jgh/core/*": ["apps/jgh/src/app/core/*"],
      "@jgh/shared/*": ["apps/jgh/src/app/shared/*"],
      "@jgh/env/*": ["apps/jgh/src/environments/*"]
    }
  },
  "files": ["src/main.ts"],
  "include": ["src/**/*.d.ts"],
  "exclude": ["jest.config.ts", "src/**/*.test.ts", "src/**/*.spec.ts"]
}
